@model IEnumerable<ProjetoSushiTime.Entidades.Usuarios>

@{
    ViewData["Title"] = "Index";
}

<h1>Usuários</h1>

<p>
    <a asp-action="Create">Novo Usuário</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Login)
            </th>
            <!-- <th>
                @Html.DisplayNameFor(model => model.Senha)
            </th> -->
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Login)
            </td>
        <!--<td>
                @Html.DisplayFor(modelItem => item.Senha)
            </td> -->
            <td>
                <a class="btn botao-outline ml-4" style="font-family: League Spartan;" asp-area="" asp-controller="Login" asp-action="Logoff">
                    Sair
                </a>
            </td>
        </tr>
}
    </tbody>
</table>
